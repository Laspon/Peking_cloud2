<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="awd.mis.servers.dao.RabbitQueuesDao">
    <resultMap id="RabbitQueuesResultMap" type="awd.mis.servers.entity.RabbitQueuesEntity">
		<result property="id" column="id" javaType="java.lang.String" jdbcType="CHAR"/>

		<result property="appid" column="appid" javaType="java.lang.String" jdbcType="VARCHAR"/>

		<result property="appname" column="appname" javaType="java.lang.String" jdbcType="VARCHAR"/>

		<result property="vhost" column="vhost" javaType="java.lang.String" jdbcType="VARCHAR"/>

		<result property="queuename" column="queuename" javaType="java.lang.String" jdbcType="VARCHAR"/>

		<result property="type" column="type" javaType="java.lang.String" jdbcType="VARCHAR"/>

		<result property="istemp" column="istemp" javaType="java.lang.String" jdbcType="VARCHAR"/>

		<result property="autodelete" column="autodelete" javaType="java.lang.String" jdbcType="VARCHAR"/>
		
    </resultMap>

    <!--用于动态生成sql所需的配置-->
    <sql id="config">
        <bind name="resultMapId" value="'RabbitQueuesResultMap'"/>
        <bind name="tableName" value="'rabbit_queues'"/>
        <bind name="resultJbxxMapId" value="'JbxxInfoResultMap'"/>
        <bind name="jbxxName" value="'JBXX'"/>
    </sql>
    
	<!--循环查找表字段是否含有rybh，有的话就自动生成jbxxList 方法，没有就不生成-->
    

    <insert id="insert" parameterType="awd.mis.servers.entity.RabbitQueuesEntity">
        <include refid="config"/>
        <include refid="BasicMapper.buildInsertSql"/>
    </insert>

    <delete id="deleteByPk" parameterType="String">
        delete from rabbit_queues where id =#{id}
    </delete>

    <delete id="delete" parameterType="awd.framework.common.entity.Entity">
        <include refid="config"/>
        <include refid="BasicMapper.buildDeleteSql"/>
    </delete>

    <update id="update" parameterType="awd.framework.common.entity.Entity">
        <include refid="config"/>
        <include refid="BasicMapper.buildUpdateSql"/>
    </update>

    <select id="query" parameterType="awd.framework.common.entity.Entity" resultMap="RabbitQueuesResultMap">
        <include refid="config"/>
        <include refid="BasicMapper.buildSelectSql"/>
    </select>

    <select id="count" parameterType="awd.framework.common.entity.Entity" resultType="int">
        <include refid="config"/>
        <include refid="BasicMapper.buildTotalSql"/>
    </select>

    <select id="sequid"  resultType="String">
        <include refid="config"/>
        <include refid="BasicMapper.buildIdSqlMysql"/>
    </select>

 </mapper>
