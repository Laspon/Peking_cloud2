<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="awd.mis.servers.dao.FxpgDao">
    <resultMap id="FxpgResultMap" type="awd.mis.servers.entity.FxpgEntity">
		<result property="id" column="id" javaType="java.lang.String" jdbcType="CHAR"/>		
		<result property="jslx" column="jslx" javaType="java.lang.String" jdbcType="CHAR"/>		
		<result property="classid" column="classid" javaType="java.lang.String" jdbcType="VARCHAR"/>		
		<result property="code" column="code" javaType="java.lang.String" jdbcType="VARCHAR"/>		
		<result property="content" column="content" javaType="java.lang.String" jdbcType="VARCHAR"/>		
		<result property="cyglfz" column="cyglfz" javaType="java.math.BigDecimal" jdbcType="DECIMAL"/>		
		<result property="fxpgfz" column="fxpgfz" javaType="java.math.BigDecimal" jdbcType="DECIMAL"/>		
		<result property="py" column="py" javaType="java.lang.String" jdbcType="VARCHAR"/>		
		<result property="wghf" column="wghf" javaType="java.math.BigDecimal" jdbcType="DECIMAL"/>		
		<result property="hfsx" column="hfsx" javaType="java.math.BigDecimal" jdbcType="DECIMAL"/>		
		<result property="isunique" column="isunique" javaType="java.lang.String" jdbcType="CHAR"/>		
		<result property="minvalue" column="minvalue" javaType="java.math.BigDecimal" jdbcType="DECIMAL"/>		
		<result property="maxvalue" column="maxvalue" javaType="java.math.BigDecimal" jdbcType="DECIMAL"/>		
		<result property="state" column="state" javaType="java.lang.String" jdbcType="CHAR"/>		
		<result property="creator" column="creator" javaType="java.lang.String" jdbcType="VARCHAR"/>		
		<result property="createtime" column="createtime" javaType="java.util.Date" jdbcType="TIMESTAMP"/>		
		<result property="updator" column="updator" javaType="java.lang.String" jdbcType="VARCHAR"/>		
		<result property="updatetime" column="updatetime" javaType="java.util.Date" jdbcType="TIMESTAMP"/>			
    </resultMap>

    <!--用于动态生成sql所需的配置-->
    <sql id="config">
        <bind name="resultMapId" value="'FxpgResultMap'"/>
        <bind name="tableName" value="'FXPG'"/>
    </sql>

    <insert id="insert" parameterType="awd.mis.servers.entity.FxpgEntity">
        <include refid="config"/>
        <include refid="BasicMapper.buildInsertSql"/>
    </insert>

    <delete id="deleteByPk" parameterType="String">
        delete from app where id =#{id}
    </delete>

    <delete id="delete" parameterType="awd.framework.common.entity.Entity">
        <include refid="config"/>
        <include refid="BasicMapper.buildDeleteSql"/>
    </delete>

    <update id="update" parameterType="awd.framework.common.entity.Entity">
        <include refid="config"/>
        <include refid="BasicMapper.buildUpdateSql"/>
    </update>

    <select id="query" parameterType="awd.framework.common.entity.Entity" resultMap="FxpgResultMap">
        <include refid="config"/>
        <include refid="BasicMapper.buildSelectSql"/>
    </select>

    <select id="count" parameterType="awd.framework.common.entity.Entity" resultType="int">
        <include refid="config"/>
        <include refid="BasicMapper.buildTotalSql"/>
    </select>  
    
    <select id="sequid"  resultType="String">
        <include refid="config"/>
        <include refid="BasicMapper.buildIdSqlMysql"/>
    </select>  
    
    
</mapper>
